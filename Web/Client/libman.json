{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "cdnjs",
      "library": "bootstrap@5.2.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.min.css"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "popper.js@2.11.6",
      "destination": "wwwroot/lib/popper.js/",
      "files": [
        "umd/popper.min.js"
      ]
    }
  ]
}